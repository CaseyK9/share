{
  "Version": "12.4.1",
  "Name": "MrdemonWolf Custom Uploader",
  "DestinationType": "ImageUploader, FileUploader",
  "RequestMethod": "POST",
  "RequestURL": "https://[YOUR_DOMAIN]/api/upload/",
  "Headers": {
    "Authorization": "Bearer [YOUR_TOKEN]"
  },
  "Body": "MultipartFormData",
  "FileFormName": "file",
  "URL": "$json:file.url$",
  "DeletionURL": "$json:file.delete$"
}
