{
  "name": "share",
  "version": "5.0.0",
  "description": "Advanced custom uploader with web front-end.  Allows uploads for  images, files, and text. Built with ShareX in mind.",
  "keywords": [
    "sharex",
    "custom-uploader",
    "uploader",
    "files",
    "images",
    "text",
    "custom"
  ],
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "lint": "eslint . --ext .js --fix",
    "format": "prettier-eslint --write $PWD/'**/*.{js,json}'",
    "test": "cross-env NODE_ENV=test mocha tests --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrDemonWolf/share.git"
  },
  "author": "Nathan Henniges",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrDemonWolf/share/issues"
  },
  "homepage": "https://mrdemonwolf.github.io/share",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "consola": "^2.14.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "filesize": "^6.1.0",
    "fs-extra": "^9.0.1",
    "helmet": "^3.23.3",
    "js-md5": "^0.7.3",
    "js-sha256": "^0.9.0",
    "js-sha512": "^0.8.0",
    "jsonwebtoken": "^8.5.1",
    "lusca": "^1.6.1",
    "moment": "^2.27.0",
    "mongoose": "^5.9.25",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nanoid": "^3.1.10",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "slugify": "^1.4.4",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "prettier-eslint-cli": "^5.0.0",
    "supertest": "^4.0.2"
