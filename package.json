{
  "name": "share",
  "version": "4.0.0",
  "description": "Advanced uploader with web front-end for images,files,and text. Built with ShareX in mind. Licensed under MIT and is free to use.",
  "keywords": [
    "sharex",
    "custom-uploader",
    "uploader",
    "files",
    "images",
    "text",
    "custom"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev-start": "nodemon src/index.js",
    "scss": "sass --watch src/scss:src/public/assets/css --style compressed",
    "dev": "concurrently -n \"App,SCSS\" \"npm run dev-start\" \"npm run scss\"",
    "lint": "eslint . --ext .js --fix",
    "format": "prettier-eslint 'server/**/*.{js,json}' --write",
    "apidoc": "apidoc -i ./src/routes/api -f .js -o ./docs",
    "test": "NODE_ENV=test mocha --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrDemonWolf/share.git"
  },
  "author": "Nathan Henniges",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrDemonWolf/share/issues"
  },
  "homepage": "https://github.com/MrDemonWolf/share#readme",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "apidoc": "^0.18.0",
    "async": "^2.6.3",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "concurrently": "^4.1.2",
    "connect-mongo": "^2.0.3",
    "consola": "^2.11.0",
    "dotenv": "^7.0.0",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-ip": "^1.0.3",
    "express-rate-limit": "^3.5.3",
    "express-session": "^1.17.0",
    "file-exists": "^5.0.1",
    "generate-password": "^1.4.2",
    "gravatar": "^1.8.0",
    "helmet": "^3.21.2",
    "js-md5": "^0.7.3",
    "jsonwebtoken": "^8.5.1",
    "lusca": "^1.6.1",
    "method-override": "^3.0.0",
    "mjml": "^4.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.10",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nanoid": "^2.1.6",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "password-validator": "^4.1.3",
    "request-ip": "^2.1.3",
    "sass": "^1.23.3",
    "slugify": "^1.3.6",
    "superagent": "^5.1.2",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "nodemon": "^1.19.4",
    "prettier-eslint-cli": "^5.0.0",
    "supertest": "^4.0.2",
    "mocha": "^6.2.2"
  },
  "engines": {
    "node": ">=8.11.3",
    "npm": ">=5.6.0"
  }
}
