{
  "name": "share",
  "version": "4.2.0",
  "description": "Advanced custom uploader with web front-end.  Allows uploads for  images, files, and text. Built with ShareX in mind.",
  "keywords": [
    "sharex",
    "custom-uploader",
    "uploader",
    "files",
    "images",
    "text",
    "custom"
  ],
  "main": "src/index.js",
  "scripts": {
    "install": "bower i",
    "start": "node src/index.js",
    "dev-start": "nodemon src/index.js",
    "scss": "sass --watch src/scss:src/public/assets/css --style compressed",
    "dev": "concurrently -n \"App,SCSS\" \"npm run dev-start\" \"npm run scss\"",
    "lint": "eslint . --ext .js --fix",
    "format": "prettier-eslint --write $PWD/'src/**/*.{js,json}'",
    "bower": "bower",
    "docs": "cd docs; bundle exec jekyll serve",
    "test": "NODE_ENV=test mocha --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrDemonWolf/share.git"
  },
  "author": "Nathan Henniges",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrDemonWolf/share/issues"
  },
  "homepage": "https://mrdemonwolf.github.io/share",
  "dependencies": {
    "@sendgrid/mail": "^7.0.1",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "bower": "^1.8.8",
    "chai": "^4.2.0",
    "compression": "^1.7.4",
    "concurrently": "^5.1.0",
    "connect-mongo": "^3.2.0",
    "consola": "^2.11.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.2",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-flash": "0.0.2",
    "express-ip": "^1.0.4",
    "express-rate-limit": "^5.1.1",
    "express-session": "^1.17.0",
    "filesize": "^6.1.0",
    "filesize-parser": "^1.5.0",
    "fs-extra": "^9.0.0",
    "generate-password": "^1.5.1",
    "helmet": "^3.22.0",
    "js-sha512": "^0.8.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "lusca": "^1.6.1",
    "method-override": "^3.0.0",
    "mjml": "^4.6.2",
    "moment": "^2.24.0",
    "mongoose": "^5.9.9",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.3",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "password-validator": "^5.0.3",
    "qrcode": "^1.4.4",
    "request-ip": "^2.1.3",
    "slugify": "^1.4.0",
    "supertest": "^5.0.0-0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "eslint": "^7.0.0-alpha.2",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-promise": "^4.2.1",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.3",
    "prettier-eslint-cli": "^5.0.0",
    "sass": "^1.26.3"
  },
  "engines": {
    "node": ">=12.15.0",
    "npm": ">=6.13.4"
  }
}
