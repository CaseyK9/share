{
  "name": "share",
  "version": "4.0.0",
  "description": "Advanced custom uploader with web front-end.  Allows uploads for  images, files, and text. Built with ShareX in mind.",
  "keywords": [
    "sharex",
    "custom-uploader",
    "uploader",
    "files",
    "images",
    "text",
    "custom"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev-start": "nodemon src/index.js",
    "scss": "sass --watch src/scss:src/public/assets/css --style compressed",
    "dev": "concurrently -n \"App,SCSS\" \"npm run dev-start\" \"npm run scss\"",
    "lint": "eslint . --ext .js --fix",
    "format": "prettier-eslint 'server/**/*.{js,json}' --write",
    "apidoc": "apidoc -i ./src/routes/api -f .js -o ./docs",
    "test": "NODE_ENV=test mocha --exit",
    "travis": "NODE_ENV=test DATABASE_URI=$DATABASE_URI TITLE=TITLE FULL_DOMAIN=$FULL_DOMAIN FOOTER_TITLE=$FOOTER_TITLE DESC=$DESC FILE_CHECK=$FILE_CHECK CREDIT=$CREDIT SHOW_VERSION=$SHOW_VERSION SIGNUPS=$SIGNUPS SIGNUP_TERMS=$SIGNUP_TERMS mocha --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrDemonWolf/share.git"
  },
  "author": "Nathan Henniges",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrDemonWolf/share/issues"
  },
  "homepage": "https://github.com/MrDemonWolf/share#readme",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "apidoc": "^0.19.0",
    "async": "^3.1.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.0.0",
    "connect-mongo": "^3.2.0",
    "consola": "^2.11.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-ip": "^1.0.3",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.17.0",
    "faker": "^4.1.0",
    "file-exists": "^5.0.1",
    "filesize": "^6.0.1",
    "fs-extra": "^8.1.0",
    "generate-password": "^1.4.2",
    "gravatar": "^1.8.0",
    "helmet": "^3.21.2",
    "js-sha512": "^0.8.0",
    "jsonwebtoken": "^8.5.1",
    "lusca": "^1.6.1",
    "method-override": "^3.0.0",
    "mjml": "^4.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.13",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nanoid": "^2.1.7",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "password-validator": "^5.0.3",
    "request-ip": "^2.1.3",
    "slugify": "^1.3.6",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "sass": "^1.23.7",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-promise": "^4.2.1",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": ">=8.11.3",
    "npm": ">=5.6.0"
  }
}
