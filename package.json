{
  "name": "share-mrdemonwolf-me",
  "version": "1.0.0",
  "description": "This a simple ShareX server so I can upload images,files, and text files on my own domain/server. This can be used for anyone but its not supported as much.",
  "keywords": [
    "sharex",
    "custom-uploader",
    "uploader",
    "files",
    "images",
    "text",
    "custom"
  ],
  "main": "index.js",
  "scripts": {
    "web": "node src/index.js",
    "dev-web": "nodemon src/index.js",
    "scss": "sass --watch src/scss:src/public/css --style compressed",
    "dev": "concurrently -n \"Web,Scss\" \"npm run dev-web\" \"npm run scss\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrDemonWolf/files.mrdemonwolf.me.git"
  },
  "author": "MrDemonWolf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrDemonWolf/files.mrdemonwolf.me/issues"
  },
  "homepage": "https://github.com/MrDemonWolf/files.mrdemonwolf.me#readme",
  "dependencies": {
    "async": "^2.6.2",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "connect-mongo": "^2.0.3",
    "consola": "^2.5.7",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.9.0",
    "dotenv": "^7.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.3-alpha.1",
    "express-flash": "0.0.2",
    "express-rate-limit": "^3.4.0",
    "express-session": "^1.15.6",
    "file-exists": "^5.0.1",
    "generate-password": "^1.4.1",
    "gravatar": "^1.8.0",
    "js-md5": "^0.7.3",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mjml": "^4.4.0-beta.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.19",
    "morgan": "^1.9.1",
    "nanoid": "^2.0.1",
    "nodemailer": "^5.1.1",
    "nodemailer-sendgrid": "^1.0.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "sass": "^1.17.3",
    "validator": "^10.11.0"
